{
  "name": "artmosphere",
  "productName": "ArtMosphere",
  "version": "0.0.2",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "npx electron .",
    "watch": "npx tsc -w main.ts",
    "build": "npx tsc main.ts && npx electron-builder -w --x64 --ia32",
    "postinstall": "npx electron-builder install-app-deps",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": "https://artmosphere.gitlab.io/electron/"
      }
    ],
    "appId": "com.faizalluthfi.kecamatan-lowokwaru",
    "files": [
      "**/*",
      "!dev-app-update.yml",
      "!angular"
    ],
    "appImage": {
      "artifactName": "${name}.${ext}"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage"
        }
      ],
      "icon": "assets/icons/png"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icons/icon.ico",
      "artifactName": "${name}_setup_${version}.${ext}"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/artmosphere/electron.git"
  },
  "author": "Faizal Luthfi <zalluth@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/artmosphere/electron/issues"
  },
  "homepage": "https://gitlab.com/artmosphere/electron#README",
  "dependencies": {
    "auto-launch": "^5.0.5",
    "bookshelf": "^0.13.3",
    "electron-is-dev": "^1.0.1",
    "electron-log": "^2.2.17",
    "electron-updater": "^3.1.2",
    "env-paths": "^1.0.0",
    "knex": "^0.15.2",
    "node-thermal-printer": "^2.0.0",
    "rmdir": "^1.2.0",
    "sqlite3": "^4.0.2",
    "targz": "^1.0.1"
  },
  "devDependencies": {
    "@types/es6-collections": "^0.5.31",
    "electron": "^3.0.3",
    "electron-reload": "^1.2.5",
    "typescript": "^3.1.1"
  }
}
